{
  "name": "devcamp-boost-2023",
  "version": "1.0.0",
  "description": "kintoneカスタマイズ開発テンプレート。kintone devCamp Boost! (2023年)用です。",
  "private": true,
  "main": "index.js",
  "scripts": {
    "postinstall": "run-s init:*",
    "build": "vite build",
    "upload": "dotenv kintone-customize-uploader ./customize-manifest.json",
    "start": "run-p -l 'build -- --watch' 'upload -- --watch'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "run-p -l lint:*",
    "lint:eslint": "eslint --ext .js,.ts,.mts ./",
    "lint:prettier": "prettier --check **/*.{json,md}",
    "fix": "run-p -l fix:*",
    "fix:eslint": "run-s 'lint:eslint -- --fix'",
    "fix:prettier": "run-s 'lint:prettier -- --write'",
    "init:manifest": "zx ./scripts/init-manifest-json.mjs",
    "init:dotenv": "zx ./scripts/init-dotenv.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mshrtsr/devCamp-Boost-2023.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "kintone"
  ],
  "author": {
    "name": "Cybozu, Inc.",
    "url": "https://cybozu.co.jp"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mshrtsr/devCamp-Boost-2023/issues"
  },
  "homepage": "https://github.com/mshrtsr/devCamp-Boost-2023#readme",
  "devDependencies": {
    "@cybozu/eslint-config": "^20.0.1",
    "@kintone/customize-uploader": "^7.0.2",
    "@kintone/dts-gen": "^7.0.3",
    "@types/node": "^20.3.2",
    "dotenv-expand": "^10.0.0",
    "eslint": "^8.43.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "zx": "^7.2.2"
  }
}
